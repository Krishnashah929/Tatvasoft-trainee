@using Helperland.Models
@model Helperland.Models.UserAddress
<div class="modal model-for-completion-order" id="myModaldeleteaddress">
   <div class="modal-dialog modal-dialog-centered">
      <div class="modal-content cancle-model">
         <!-- Modal Header -->
         <div class="modal-header">
            <p class="modal-title cancle-booking-title"
               id="popup-loginLabel">
               <b>
                  Delete address
               </b>
            </p>
            <button type="button" class="btn-close"
                    data-bs-dismiss="modal"></button>
         </div>
         <!-- Modal body -->
         <div class="modal-body book-with-cost-popup">
            <div class="row">
               <div class="col-auto">
                  <p class="book-with-cost-p1-popup">
                     Are you sure you want to delete this address?
                  </p>
               </div>
            </div>
            <br>
            <button type="button" class="btn extinguish-btn-setting" onclick="delexistadd()" data-bs-dismiss="modal">
               Delete
            </button>
            <input type="hidden" class="form-control" id="id" value="@Model.AddressId" />
         </div>
      </div>
   </div>
</div>
